{
  "extends": [
    "eslint:recommended",
    "@eslint/js/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es2022": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error"
  },
  "globals": {
    "React": "readonly"
  }
}